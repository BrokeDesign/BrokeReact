{
  "name": "@brokeds/broke-react",
  "version": "0.1.0",
  "description": "BrokeDS React Component Library",
  "main": "dist/brokeReact.js",
  "scripts": {
    "compile:sass": "node-sass src/styles/scss/styles.scss src/styles/css/styles.css",
    "watch:sass": "nodemon -e scss -x \"npm run compile:sass\"",
    "dev": "webpack-dev-server --mode development --open --hot",
    "start": "concurrently --kill-others \"npm run watch:sass\" \"npm run dev\"",
    "build": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrokeDesign/BrokeReact.git"
  },
  "keywords": [
    "brokeds",
    "react",
    "components",
    "design",
    "front-end",
    "reactjs"
  ],
  "author": "Christian Gorke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BrokeDesign/BrokeReact/issues"
  },
  "homepage": "https://github.com/BrokeDesign/BrokeReact#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^6.1.0",
    "img-loader": "^3.0.1",
    "interpolate-html-plugin": "^3.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "prop-types": "^15.7.2",
    "react-router": "^4.4.0",
    "react-router-dom": "^4.4.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
},
"dependencies": {
  "react": "^16.8.4",
  "react-dom": "^16.8.4"
}
}
